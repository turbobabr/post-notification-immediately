{
  "name": "post-notification-immediately",
  "version": "0.1.1",
  "description": "A helper Electron & Node.js module to post notifications immediately via macOS distributed notification center",
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "cd obj-c && xcodebuild archive -derivedDataPath $(mktemp -d) -scheme bin",
    "prepublish": "npm run build"
  },
  "files": [
    "index.js",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbobabr/post-notification-immediately.git"
  },
  "keywords": [
    "node",
    "electron",
    "post notification",
    "Distributed Notifcation Center",
    "macos"
  ],
  "author": "Andrey Shakhmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/turbobabr/post-notification-immediately/issues"
  },
  "homepage": "https://github.com/turbobabr/post-notification-immediately#readme"
}
